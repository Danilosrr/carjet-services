// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  password  String
  @@map("users")
}

model Branch {
  id        Int     @id @default(autoincrement())
  name      String  @unique
  location  Json   
  @@map("branches") 
}

model Provider {
  id        Int     @id @default(autoincrement())
  name      String  @unique
  location  Json   
  @@map("providers") 
}